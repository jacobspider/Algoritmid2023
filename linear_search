def linear_search(arr, target):
    for i in range(len(arr)):
        if arr[i] == target:
            return i  # Element leitud, tagasta indeks
    return -1  # Elementi ei leitud

# NÃ¤ide kasutamisest:
arr = [1, 2, 4, 7, 8, 13, 16, 21]
target = 21
result = linear_search(arr, target)
if result != -1:
    print(f"Element {target} leitud indeksilt {result}.")
else:
    print(f"Element {target} puudub massiivist.")
